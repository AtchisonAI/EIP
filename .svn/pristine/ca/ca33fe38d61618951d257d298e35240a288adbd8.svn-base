        case "warning":
            jNotify(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                TimeShown: time, // 显示时间：毫秒
                ShowOverlay: false, // 是否显示遮罩层 
                onClosed: closed
            });
            break;
        case "error":
            jError(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                TimeShown: time, // 显示时间：毫秒
                ShowOverlay: false, // 是否显示遮罩层 
                onClosed: closed
            });
            break;
        case "wait":
            jWait(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                ShowTimeEffect: 1,
                TimeShown: time,
                autoHide: false, // 是否自动隐藏提示条 
                ShowOverlay: true, // 是否显示遮罩层 
                onClosed: closed
            });
            $("#jWait").css({ opacity: 1, top: 5 });
            break;
        default:
    }
}

