//赋予值
function setDicValue() {
    var dicObj = art.dialog.data('dictionaryObj')[0];
    var code = dicObj.code,
        id = dicObj.id,
        tree = dicObj.tree;
    var codeObj = $('.button_search[plugin-code="' + code + '"]');
    //如果大于1则表示有多个则根据Id获取
    if (codeObj.length > 1) {
        var clickObj = $("#" + id);
        if (tree.length === 0) {
            clickObj.prev().val("");
            clickObj.prev().prev().val("");
            return;
        } else {
            clickObj.prev().val(tree[0].id);
            clickObj.prev().prev().val(tree[0].name);
        }
    }
    else {
        if (tree.length === 0) {
            codeObj.prev().val("");
            codeObj.prev().prev().val("");
            return;
        } codeObj.prev().val(tree[0].id);
        codeObj.prev().prev().val(tree[0].name);
    }
}

//赋予值
function setDicAllValue() {
    var dataAll = art.dialog.data('dictionaryObj');
    var dicObj = dataAll[0];
    var clickObj = $("#" + dataAll.formId);
    if (dicObj.id == null) {
        clickObj.prev().val("");
        clickObj.prev().prev().val("");
        return;
    } else {
        clickObj.prev().val(dicObj.id);
        clickObj.prev().prev().val(dicObj.name);
    }
}