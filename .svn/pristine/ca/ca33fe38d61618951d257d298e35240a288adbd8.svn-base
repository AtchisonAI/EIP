/*=====================常用工具类=====================*/
/**
 * 作用:根据传入的键值获取浏览器地址中参数
 * 参数:参数名称键
 */
function UtilGetUrlParam(name) {
    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
    var result = window.location.search.substr(1).match(reg);
    return result ? decodeURIComponent(result[2]) : "";
};

/**
 * 作用:重新加载该页面
 * 参数:无
 */
function UtilReloadPage() {
    window.location.reload();
}

/**
 * 作用:返回一个GUID值
 * 参数:无
 */
function UtilNewGuid() {
    var guid = "";
    for (var i = 1; i <= 32; i++) {
        var n = Math.floor(Math.random() * 16.0).toString(16);
        guid += n;
        if ((i == 8) || (i == 12) || (i == 16) || (i == 20))
            guid += "-";
    }
    return guid;
};
/**
 * 作用:获取当前窗体高度宽度
  */
function UtilWindowHeightWidth() {
    var winW, winH;
    if (window.innerHeight) { // all except IE
        winW = window.innerWidth;
        winH = window.innerHeight;
    } else if (document.documentElement && document.documentElement.clientHeight) { // IE 6 Strict Mode
        winW = document.documentElement.clientWidth;
        winH = document.documentElement.clientHeight;
    } else if (document.body) { // other
        winW = document.body.clientWidth;
        winH = document.body.clientHeight;
    }
    return { WinW: winW, WinH: winH };
};

/**
 * 作用:编码地址
  */
function UtilDecodeUri(uri) {
    if (!uri || $.trim(uri).length == 0) {
        return "";
    }
    try {
        return decodeURI(uri);
    }
    catch (e) {
        return uri;
    }
}

/**
 * 作用:Jquery的Post ajax提交,具有等待提示,但是没有阻挡层
 * 参数:url(提交地址),postData(提交数据),success(成功后触发方法)
 */
function UtilAjaxPostWaitHiddenOverlay(url, postData, success) {
    top.jWait(Language.common.wait, {
        VerticalPosition: "top",
        HorizontalPosition: "center",
        ShowTimeEffect: 1,
        TimeShown: 999999999,
        autoHide: false, // 是否自动隐藏提示条 
        ShowOverlay: false// 是否显示遮罩层 
    });
    $("#jWait").css({ opacity: 1, top: 5 });
    $.post(url, postData, function (data) {
        DialogCloseTipsMsgWait();
        success(data);
    }, "json").success(function () {//成功
    }).error(function () {//失败
        DialogCloseTipsMsgWait();
        DialogTipsMsgError(Language.common.ajaxposterror);
    }).complete(function () {//完成
    });
};

