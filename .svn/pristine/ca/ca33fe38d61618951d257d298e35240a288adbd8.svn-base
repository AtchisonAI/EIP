};
/**
 * 作用:Jquery的Post ajax提交,无等待提示
 * 参数:url(提交地址),postData(提交数据),success(成功后触发方法)
 */
function UtilAjaxPost(url, postData, success) {
    $.ajax({
        url: url, // 跳转到地址    
        data: postData,
        type: "post",
        async: false,
        dataType: "json",
        success: success,
        error: function () {
            DialogTipsMsgError(Language.common.ajaxposterror);
        }
    });
};

