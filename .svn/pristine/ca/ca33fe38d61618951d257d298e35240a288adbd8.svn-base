/**
 * 作用:顶部显示信息
 * 参数:msg(消息),time(显示时间),type(类型:success,warning,error)
 */
function DialogShowTopMsg(msg, time, type) {
    switch (type) {
        case "success":
            jSuccess(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                TimeShown: time, // 显示时间：毫秒
                ShowOverlay: false // 是否显示遮罩层 
            });
            break;
        case "warning":
            jNotify(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                TimeShown: time, // 显示时间：毫秒
                ShowOverlay: false // 是否显示遮罩层 
            });
            break;
        case "error":
            jError(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                TimeShown: time, // 显示时间：毫秒
                //autoHide: false,                // 是否自动隐藏提示条 
                ShowOverlay: false // 是否显示遮罩层 
            });
            break;
        case "wait":
            jWait(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                ShowTimeEffect: 0,
                TimeShown: time,
                autoHide: true, // 是否自动隐藏提示条 
                ShowOverlay: true // 是否显示遮罩层 
            });
            $("#jWait").css({ opacity: 1, top: 5 });
            break;
        default:
    }
}

/**
 * 作用:顶部显示信息:带关闭事件
 * 参数:msg(消息内容),time(显示时间),type(类型:success,warning,error),closed(关闭事件方法)
 */
function DialogTopMsgOnClosed(msg, time, type, closed) {
    switch (type) {
        case "success":
            jSuccess(msg, {
                VerticalPosition: "top",
                HorizontalPosition: "center",
                TimeShown: time, // 显示时间：毫秒
                ShowOverlay: false, // 是否显示遮罩层 
