var ResultSign = {}; //结果类型
ResultSign.Sucess = "success";
ResultSign.Error = "error";
ResultSign.Warn = "warning";
ResultSign.Wait = "wait";

/**
 * 作用:ajax提交后显示到界面的消息
 * 参数:data返回的结果数据
 */
function DialogAjaxResult(data) {
    var result = true;
    var resultType = data.ResultSign;
    var resultMsg = data.Message;
    switch (resultType) {
        case 0:
            DialogTipsMsgSuccess(resultMsg, 1000);
            break;
        case 1:
            DialogTipsMsgWarn(resultMsg, 2000);
            result = false;
            break;
        case 2:
            //  DialogTipsMsgError(resultMsg, 2000);
            ArtDialogContent(resultMsg);
            result = false;
            break;
        default:
    }
    return result;
};

/**
 * 作用:ajax提交后显示到界面的消息
 * 参数:data返回的结果数据
 */
function DialogAjaxArtDialogResult(data) {
    var result = true;
    var resultType = data.ResultSign;
    var resultMsg = data.Message;
    switch (resultType) {
        case 0:
            ArtDialogContent("success", resultMsg);
            break;
        case 1:
            ArtDialogContent("warning", resultMsg);
            result = false;
            break;
        case 2:
            ArtDialogContent("error", resultMsg);
            result = false;
            break;
        default:
    }
    return result;
}

