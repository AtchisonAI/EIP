<!DOCTYPE html>

<html lang="zh-cn" data-require="test-list">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="/Scripts/require.min.js" data-main="/Scripts/main.js"></script>
    @*图标库*@
    <link href="@ViewBag.Path/Contents/images/icons.css" rel="stylesheet"/>
    @*基础样式*@
    <link href="@ViewBag.Path/Contents/common.css" rel="stylesheet" />
</head>
<body>
    <div id="uiCenter" class="ui-layout-center" style="border-style: none">
        <table id="list"></table>
        <div id="pager"></div>
    </div>

    <div id="uiNorth" class="ui-layout-north" style="border-color: #ccc; border-style: solid; border-width: 1px">
        <div class="tools_box">
            @{ Html.RenderPartial("~/Areas/Common/Views/Shared/_PartialButton.cshtml");}
            <div class="select_box">
                登录时间：<input id="SearchOperateTime" class="Wdate" type="text" onclick=" WdatePicker() " />
                登录名：<input id="SearchCode" type="text" />
                真实姓名：<input id="SearchName" type="text" />
                <input type="submit"  value="搜 索" onclick=" getGridData(); " class="btnSearch">
            </div>
        </div>
    </div>
</body>
</html>
