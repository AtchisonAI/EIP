@using EIP.Common.Entities
@using EIP.Common.Entities.Dtos
@using EIP.Web.DataUsers.Helpers

@{
    ViewBag.Title = "Product Indices";
    ViewBag.Require = "reporting-ProductIndices-queryinfo";
    Layout = "~/Views/Shared/_LayoutPageBaseList.cshtml";
}
<div class="container-fluid">

    <div class="row">
        <div class="col-lg-12">
            <div class="panel-group" id="accordion">
                <div class=" panel panel-primary">
                    <div class="panel-heading text-center">
                        <h3 class="panel-title">
                            <a data-toggle="collapse" data-parent="#accordion" href="#condition">
                                Production Indices
                            </a>
                        </h3>
                    </div>
                    <div id="condition" class="panel-collapse collapse in">
                        <div class="panel-body">
                            <form class="form-horizontal">

                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="col-sm-4 text-right">
                                            <label for="startDate" class="control-label text-right"><span class="need">*</span>查询区间起:</label>
                                        </div>
                                        <div class="col-sm-8 text-left">
                                            <input id="StartTime" type="text" name="StartTime" readonly style="background-color:white;" class="form-control manualfilter text-left" onfocus="WdatePicker({dateFmt:'yyyyMMdd',readOnly:true})" />
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="col-sm-4 text-right">
                                            <label for="EndDate" class="control-label text-right"><span class="need">*</span>查询区间起止:</label>
                                        </div>
                                        <div class="col-sm-8 text-left">
                                            <input id="EndTime" type="text" name="EndTime" style="background-color:white;" readonly class="form-control manualfilter text-left" onfocus="WdatePicker({dateFmt:'yyyyMMdd',readOnly:true})" /><br>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-10">
                                    </div>
                                    <div class="col-lg-2">
                                        <div class="col-sm-6">
                                            <input type="button" name="btn_select_box" value="搜 索" class="btn btn-primary pull-right" />
                                        </div>
                                        <div class="col-sm-6">
                                            <input type="button" name="btn_reset_box" value="重 置" class="btn pull-right">
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <h3 class="panel-title">Production Indices</h3>
                </div>

                <ul id="myTab" class="nav nav-pills">
                    <li class="active">
                        <a href="#home" data-toggle="tab">Production</a>
                    </li>
                    <li>
                        <a href="#page1" data-toggle="tab">Engineering</a>
                    </li>
                    <li>
                        <a href="#page2" data-toggle="tab">Backup</a>
                    </li>
                    <li>
                        <a href="#page3" data-toggle="tab">NPW</a>
                    </li>
                    <li>
                        <a href="#page4" data-toggle="tab">SubTotal</a>
                    </li>
                </ul>

                <div id="myTabContent" class="tab-content">

                    <div class="tab-pane fade in active" id="home">
                        <div class="panel-body" id="listpnlbody">
                            <div class="col-lg-12">
                                <table id="list" class="grid"></table>
                                <div id="pager"></div>
                            </div>
                        </div>
                    </div>

                    <div class="tab-pane fade" id="page1">
                        <div class="panel-body" id="listpnlbody">
                            <div class="col-lg-12">
                                <table id="list1" class="grid"></table>
                                <div id="pager1"></div>
                            </div>
                        </div>
                    </div>

                    <div class="tab-pane fade" id="page2">
                        <div class="panel-body" id="listpnlbody">
                            <div class="col-lg-12">
                                <table id="list2" class="grid"></table>
                                <div id="pager2"></div>
                            </div>
                        </div>
                    </div>

                    <div class="tab-pane fade" id="page3">
                        <div class="panel-body" id="listpnlbody">
                            <div class="col-lg-12">
                                <table id="list3" class="grid"></table>
                                <div id="pager3"></div>
                            </div>
                        </div>
                    </div>

                    <div class="tab-pane fade" id="page4">
                        <div class="panel-body" id="listpnlbody">
                            <div class="col-lg-12">
                                <table id="list4" class="grid"></table>
                                <div id="pager4"></div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="panel-body" id="listpnlbody">
                    <div class="col-lg-12">
                        <table id="list" class="grid"></table>
                        <div id="pager"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>